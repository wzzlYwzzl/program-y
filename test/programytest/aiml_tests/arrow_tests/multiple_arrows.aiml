<aiml>

    <!-- Not priority word -->

    <category>
        <pattern>^ ADEBUG1 ^ ADEBUG2 ^</pattern>
        <template>AFOUND1</template>
    </category>

    <category>
        <pattern>^ ASEPARATOR ^</pattern>
        <template>AFOUND2</template>
    </category>

    <!-- Priority word -->

    <category>
        <pattern>^ $BDEBUG1 ^ $BDEBUG2 ^</pattern>
        <template>BFOUND1</template>
    </category>

    <category>
        <pattern>^ BSEPARATOR ^</pattern>
        <template>BFOUND2</template>
    </category>

</aiml>